#include<iostream>
using namespace std;

int main()
{
  int n;
  cout<<"enetr the size of the array"<<endl;
  cin>>n;
  int a[n];
  cout<<"enter the elements of thr array"<<endl;
  //input 
  for(int i=0;i<n;i++)
  {
    cin>>a[i] ;
  }
  
  //OUTPUT
  for(int i=0;i<n;i++)
  {
    cout<<a[i]<<endl;
  }
  return 0;
}
